{
  "compilerOptions": {
    "outDir": "./dist/",
		"sourceMap": true,
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"moduleResolution": "node"
  },
	"include": [
			"./app/**/*"
	],
	"exclude": [
		"./node_modules/**/*"
	]
}
